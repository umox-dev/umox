{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "arrowParens": "always",
  "trailingComma": "all",
  "printWidth": 100,
  "overrides": [
    {
      "files": ["**/.eslintrc"],
      "options": {
        "parser": "json"
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
